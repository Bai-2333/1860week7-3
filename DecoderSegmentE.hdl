CHIP DecoderSegmentE {
    IN D, C, B, A;
    OUT e;

    PARTS:
    Not(in=A, out=notA);
    Not(in=B, out=notB);
    Not(in=C, out=notC);
    Not(in=D, out=notD);

    And(a=A, b=C, out=AC);
    And(a=notA, b=notB, out=notAnotB);
    And(a=notC, b=notD, out=notCnotD);
    And(a=C, b=D, out=CD);
    And(a=A, b=B, out=AB);

    Or(a=AC, b=notAnotB, out=term1);
    Or(a=term1, b=notCnotD, out=term2);
    Or(a=term2, b=CD, out=term3);
    Or(a=term3, b=AB, out=e);
}